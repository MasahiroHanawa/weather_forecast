{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "installedESLint": true,
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "experimentalObjectRestSpread": true
  },
  "devDependencies": true,
  "globals": {
    "document": true
  },
  "es6": true,
  "rules": {
    "semi": [2, "always"],
    "strict": 1,
    "no-undef" : 2
  }
}