{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "installedESLint": true,
  "plugins": [
    "react",
    "flowtype"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "experimentalObjectRestSpread": true
  },
  "devDependencies": true,
  "globals": {
    "document": true
  },
  "es6": true,
  "rules": {
    "semi": [2, "always"],
    "strict": 1,
    "no-undef" : 2,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "flowtype/define-flow-type": 1,
    "flowtype/use-flow-type": 1
  }
}